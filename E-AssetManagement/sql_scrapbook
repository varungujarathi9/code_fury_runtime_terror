CREATE TABLE USERSS (
		UNIQUEID INTEGER NOT NULL,
		NAME VARCHAR(100),
		TELEPHONENUMBER VARCHAR(100),
		ROLE VARCHAR(100),
		EMAILID VARCHAR(100),
		USERNAME VARCHAR(100),
		PASSWORD VARCHAR(100),
		LASTLOGIN DATE
	);
	
CREATE TABLE ASSETS (
		ASSET_ID INTEGER NOT NULL,
		ASSET_NAME VARCHAR(100),
		ASSET_TYPE VARCHAR(100),
		DESCRIPTION VARCHAR(100),
		DATE_ADDED DATE,
		IS_AVAILABLE BOOLEAN,
		ADMIN_ALERT BOOLEAN
	);	

CREATE TABLE ISSUELOG (
		ASSET_ID INTEGER NOT NULL,
		UNIQUEID INTEGER NOT NULL,
		ISSUE_DATE DATE,
		RETURN_DATE DATE,
);		
	
ALTER TABLE ISSUELOG
ADD FOREIGN KEY (ASSET_ID) REFERENCES ASSETS(ASSET_ID);

ALTER TABLE ISSUELOG
ADD FOREIGN KEY (UNIQUEID) REFERENCES USERSS(UNIQUEID);

Insert into ASSETS values(234,'LAPTOP12','LAPTOP','sadf','2020-03-03','0','0');

Insert into ASSETS values(2123,'MOBILE123','MOBILE','sadfqw','2020-05-03','1','0');

DROP TABLE ASSETS;
	